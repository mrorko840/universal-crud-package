{
    "name": "hashtag/universal-crud",
    "description": "Generic Laravel CRUD + file upload API for any table.",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Hemel Mridha",
        "email": "youremail@example.com"
      }
    ],
    "require": {
      "php": ">=8.1",
      "illuminate/support": "^10.0|^11.0",
      "illuminate/routing": "^10.0|^11.0",
      "illuminate/database": "^10.0|^11.0",
      "illuminate/filesystem": "^10.0|^11.0"
    },
    "autoload": {
      "psr-4": {
        "Hashtag\\UniversalCrud\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Hashtag\\UniversalCrud\\UniversalCrudServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
  